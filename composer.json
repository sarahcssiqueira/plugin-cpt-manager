{
  "name": "sarahcssiqueira/custom-post-manager",
  "description": "Plugin to manage custom posts types for your WordPress project",
  "license": "GPL-2.0+",
  "type": "wordpress-plugin",
  "autoload": {
    "psr-4": {
      "CustomPostManager\\Inc\\": "./"
    }
  },
  "authors": [
    {
      "name": "sarahcssiqueira",
      "email": "sarahcosiqueira@gmail.com",
      "homepage": "https://sarahjobs.com"
    }
  ],
  "support": {
    "src": "https://github.com/sarahcssiqueira/plugin-cpt-manager",
    "issues": "https://github.com/sarahcssiqueira/plugin-cpt-manager/issues"
  },
  "require": {},
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^2.3",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "scripts": {
    "cstd": "./vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
    "cs": "./vendor/bin/phpcs --standard=.phpcs.xml --report=summary .",
    "cbf": "./vendor/bin/phpcbf --standard=.phpcs.xml ."
  }
}
